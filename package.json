{
  "name": "bddstepbrowser",
  "productName": "BDDStepBrowser",
  "version": "1.0.0",
  "description": "BDD test feature, scenario and step brwoser",
  "license": "MIT",
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "keywords": [
    "bdd",
    "test",
    "software quality assurance",
    "gherkin",
    "cucumber",
    "jbehave"
  ],
  "author": {
    "name": "Attila Gyori",
    "email": "attila.gyori.ag@gmail.com",
    "url": "https://attila-gyori.com"
  },
  "browserslist": [
    "last 4 versions"
  ],
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./release-builds --overwrite --platform=darwin --arch=x64 --icon=assets/icon.icns"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "dotenv": "^16.0.0",
    "gherkin-parse": "^1.0.6",
    "match-sorter": "^1.8.1",
    "moment": "^2.25.3",
    "mongoose": "^5.9.13",
    "namor": "^1.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^17.0.2",
    "react-moment": "^0.9.7",
    "react-select": "^1.2.1",
    "react-table": "^7.7.0",
    "use-local-storage": "^2.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.2.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
